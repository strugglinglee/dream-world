{
    "*.{js,ts,tsx,jsx,vue}": [
        "eslint --fix",
        "prettier --write",
        "git add"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
        "prettier --write--parser json"
    ],
    "package.json": [
        "prettier --write"
    ],
    "*.md": [
        "prettier --write"
    ]
}